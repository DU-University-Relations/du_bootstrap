{% set data_category = custom.tags %}
<div class="events-listing__item {{ data_category }}">
    {% embed "@du_bootstrap/misc/event-card.twig" %}
        {% set date = custom.start_date %}
        {% set start_time = custom.start_time %}
        {% set end_time = custom.end_time %}
        {% set location = custom.location %}
        {% set title = fields.title.content %}
        {% set url = custom.url %}
    {% endembed %}
</div>